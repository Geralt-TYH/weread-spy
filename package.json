  "author": "magicdawn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/magicdawn/weread-spy/issues"
  },
  "homepage": "https://github.com/magicdawn/weread-spy#readme",
  "devDependencies": {
    "@magicdawn/eslint-config": "latest",
    "@magicdawn/prettier-config": "latest",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.10.1",
    "@types/puppeteer": "^3.0.2",
    "codecov": "^3.3.0",
    "eslint": "^6.8.0",
    "husky": "latest",
    "lint-staged": "latest",
    "mocha": "^6.1.4",
    "nyc": "^14.0.0",
    "prettier": "latest",
    "should": "^13.2.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
