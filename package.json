{
  "name": "weread-spy",
  "description": "weread spy",
  "version": "0.0.1",
  "author": "magicdawn",
  "bin": "bin/weread-spy.js",
  "types": "lib/index.d.ts",
  "bugs": {
    "url": "https://github.com/magicdawn/weread-spy/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "homepage": "https://github.com/magicdawn/weread-spy#readme",
  "keywords": [
    "download",
    "weread"
  ],
  "scripts": {
    "dev": "tsc -w --incremental",
    "build": "rm -rf lib; rm tsconfig.tsbuildinfo; tsc",
    "gen-readme": "swig render ./readme/readme.md > README.md && prettier --write README.md",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "report-cover": "codecov",
    "test": "mocha",
    "test-cover": "nyc --reporter=lcov --reporter=text mocha",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/weread-spy.git"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "clipanion": "^3.1.0",
    "comlink": "^4.3.1",
    "debug": "^4.3.3",
    "dl-vampire": "^0.5.0",
    "env-paths": "^2.2.1",
    "execa": "^4.1.0",
    "filenamify": "^4.3.0",
    "fs-extra": "^9.1.0",
    "globby": "^10.0.2",
    "inquirer": "^7.3.3",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "mime": "^2.6.0",
    "mimetype": "^0.0.8",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.7",
    "nunjucks": "^3.2.3",
    "promise.map": "^0.1.0",
    "puppeteer": "^5.5.0",
    "sharp": "^0.26.3",
    "swig-templates": "^2.0.3",
    "tslib": "^1.14.1",
    "umi-request": "^1.4.0",
    "urijs": "^1.19.10"
  },
  "devDependencies": {
    "@magicdawn/eslint-config": "^0.0.6",
    "@magicdawn/prettier-config": "^0.0.2",
    "@swc/core": "^1.2.152",
    "@swc/helpers": "^0.3.6",
    "@tsconfig/node12": "^1.0.9",
    "@types/chai": "^4.3.0",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/inquirer": "^7.3.3",
    "@types/jake": "^0.0.33",
    "@types/lodash": "^4.14.179",
    "@types/mime": "^2.0.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "@types/node-fetch": "^2.6.1",
    "@types/nunjucks": "^3.2.1",
    "@types/prettier": "^2.4.4",
    "@types/puppeteer": "^3.0.7",
    "@types/sharp": "^0.26.1",
    "@types/urijs": "^1.19.19",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^6.2.3",
    "prettier": "^2.7.1",
    "should": "^13.2.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "pkg": {
    "assets": [
      "assets/templates/"
    ],
    "scripts": [
      "lib/**/*.worker.js"
    ]
